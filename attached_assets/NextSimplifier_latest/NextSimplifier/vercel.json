{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "framework": null,
  "rewrites": [
    { "source": "/api/passport/:slug", "destination": "/api/passport/:slug" },
    { "source": "/api/projects/:slug", "destination": "/api/projects/:slug" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
